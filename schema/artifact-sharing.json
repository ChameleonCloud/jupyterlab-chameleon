{
  "jupyter.lab.setting-icon-class": "jp-TextEditorIcon",
  "jupyter.lab.setting-icon-label": "Chameleon: Sharing",
  "title": "Chameleon: Sharing",
  "description": "Settings for the experiment packaging and sharing on Chameleon.",
  "properties": {
    "externalBaseUrl": {
      "type": "string",
      "title": "Base URL for external editing of artifacts",
      "default": "https://www.chameleoncloud.org/experiment/share"
    },
    "externalDetailEndpoint": {
      "type": "string",
      "title": "Endpoint for viewing artifact",
      "default": "/{externalId}"
    },
    "externalCreateEndpoint": {
      "type": "string",
      "title": "Endpoint for creating new artifacts",
      "default": "/create/embed?deposition_id={depositionId}&deposition_repo={depositionRepo}"
    },
    "externalUpdateEndpoint": {
      "type": "string",
      "title": "Endpoint for updating existing artifacts",
      "default": "/{externalId}/edit/embed"
    },
    "externalNewVersionEndpoint": {
      "type": "string",
      "title": "Endpoint for adding new version to existing artifact",
      "default": "/{externalId}/edit/embed?new_version&deposition_id={depositionId}&deposition_repo={depositionRepo}"
    }
  },
  "additionalProperties": false,
  "type": "object"
}
